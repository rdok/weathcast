{
  "name": "weathcast",
  "license": "ISC",
  "version": "1.0.0",
  "description": "",
  "main": "src/main.js",
  "scripts": {
    "start": "node src/main.js",
    "start:dev": "ENV=dev nodemon src/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "linter": "eslint src/**/*.js __tests__/**/*.js",
    "prettier": "yarn prettier:js-css && yarn prettier:views",
    "prettier:js-css": "prettier --check **/*.js **/*.css",
    "prettier:views": "prettier --parser glimmer --check views/**/*.hbs",
    "prettier:fix": "yarn prettier:fix:js-css && yarn prettier:fix:views",
    "prettier:fix:js-css": "npx prettier --write **/*.js **/*.css",
    "prettier:fix:views": "npx prettier --parser glimmer --write views/**/*.hbs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdok/weathcast.git"
  },
  "keywords": [],
  "author": "Rizart Dokollari",
  "bugs": {
    "url": "https://github.com/rdok/weathcast/issues"
  },
  "homepage": "https://github.com/rdok/weathcast#readme",
  "dependencies": {
    "express": "^4.17.1",
    "hbs": "^4.1.2",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^26.6.3",
    "nock": "^13.0.10",
    "nodemon": "^2.0.7",
    "prettier": "2.2.1"
  },
  "engines": {
    "yarn": "1.22.x"
  }
}
